      <div class="body">
       <%
        if(data.length!=0)
        {                 
       %>
   <form action="/action" name="f1" method="post">
     <table  class=" table-bordered" bordercolor="black" width="90%" align="center" cellspacing="10" cellpadding="10">
      
       
      <tr style="height:50px">
             <th style="text-align: center; background-color:#22b022">Sl No</th>
             <th style="text-align: center; background-color:#22b022">Student Name</th>
             <th style="text-align: center; background-color:#22b022">Email id</th>
             <th style="text-align: center; background-color:#22b022">Contact No</th>
             <th style="text-align: center; background-color:#22b022">Subject</th>
             <th style="text-align: center; background-color:#22b022">Joining Date</th>
             <th style="text-align: center; background-color:#22b022" colspan="2">Edit/Delete</th>
             
      </tr>
       <%
       var i=1
       data.forEach((e)=>{
       %>
           <tr>
               <td class="danger" style="text-align: center;"><%=i%></td>
               <td style="text-align: center;"><%=e.reg %></td>
               <td style="text-align: center;"><%=e.email%></td>
               <td style="text-align: center;"><%=e.contact%></td>
               <td style="text-align: center;"><%=e.sub%></td>
               <td style="text-align: center;"><%=e.date%></td>
               
               <td style="text-align: center;"><a href="/edit?id=<%=e._id%>"><i class="fa fa-edit" style="color:green !important ;"></i></a></td>
               <td style="text-align: center;"><a href="/delete?id=<%=e._id%>"><i class="fa fa-trash" style="color:red !important ;"></i></a></td>
           </tr>
     <%
       i++;
       })
       %>
    </table>
   </form>
